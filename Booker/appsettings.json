{
  "ConnectionStrings": {
    "DefaultConnection": "{#db-connection-string#}",
    "BookerIdentityDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=Booker;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "DatabaseSettings": {
    "ClearDatabaseOnStartup": false
  },
  "AzureStorage": {
    "ConnectionString": "{#storage-connection-string#}",
    "ContainerName": "{#container-name#}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "SmtpSettings": {
    "Server": "smtp.webio.pl",
    "Port": 587,
    "Username": "{smtp-username}",
    "Password": "#{smtp-password}#"
  },
  "SassCompiler": {
    "Source": "wwwroot/scss",
    "IncludePaths": [
      "wwwroot/lib/pico/scss"
    ],
    "Arguments": "--style=compressed --no-source-map",
    "Configurations": {
      "Debug": {
        "Arguments": "--style=expanded"
      }
    }
  },
  "AllowedHosts": "*"
}